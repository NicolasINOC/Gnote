<h1> Inscription </h1>

<%= form_for :user, url: users_path do |f| %>
  <p>
    <%= f.label :email %><br>
    <%= f.text_field :email %>
  </p>

  <p>
    <%= f.label :firstName %><br>
    <%= f.text_field :firstName %>
  </p>

  <p>
    <%= f.label :lastName %><br>
    <%= f.text_field :lastName %>
  </p>

  <p>
    <%= f.label :secretHash %><br>
    <%= f.text_field :secretHash %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>
